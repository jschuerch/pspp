| redPoints iter rand x y pi |redPoints := 0.iter := 100000000.Transcript clear.1 to: iter do: 	[ :i |		rand := Random new.		x := (rand next).		y := (rand next).		(x squared + y squared) < 1			ifTrue: [ redPoints := redPoints + 1 ].		"Transcript show: redPoints asString, ' '; show: (x sqrt + y sqrt) asString; show: x asString, ' '; show: y asString; cr" 	].pi := (redPoints / iter) asFloat * 4.Transcript show: redPoints; cr; show: iter; cr; show: pi; cr.